<resources>
    <color name="color_black">#000000</color>
    <color name="color_insektionen">#44687D</color>
    <color name="color_insektionen_light">#70AACC</color>

    <style name="AgeCheckLargeText">
        <item name="android:textColor">@android:color/primary_text_light</item>
        <item name="android:textSize">36sp</item>
    </style>

    <style name="AgeCheckSmallText">
        <item name="android:textColor">@android:color/primary_text_light</item>
    </style>

    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Global app theme -->
    </style>

    <style name="Divider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">?android:attr/listDivider</item>
    </style>

    <style name="EventAttendeesListPrimary" parent="TextAppearance.AppCompat.Small">
        <item name="android:ellipsize">end</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@android:color/primary_text_light</item>
    </style>

    <style name="EventInfoHeader" parent="TextAppearance.AppCompat.Headline">
        <item name="android:textColor">@android:color/primary_text_light</item>
    </style>

    <style name="EventInfoMedium" parent="TextAppearance.AppCompat.Medium">
        <item name="android:textColor">@android:color/primary_text_light</item>
    </style>

    <style name="EventsListDate" parent="TextAppearance.AppCompat.Medium">
        <item name="android:textColor">@android:color/primary_text_light</item>
        <item name="android:gravity">right</item>
    </style>

    <style name="EventsListPrimary" parent="TextAppearance.AppCompat.Medium">
        <item name="android:ellipsize">end</item>
        <item name="android:textColor">@android:color/primary_text_light</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="EventsListSecondary" parent="TextAppearance.AppCompat.Small">
        <item name="android:ellipsize">end</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="MemberInfoHeader" parent="TextAppearance.AppCompat.Headline">
        <item name="android:textColor">@android:color/primary_text_light</item>
    </style>

    <style name="MemberInfoSubHeader" parent="TextAppearance.AppCompat.Medium">
    </style>

    <style name="MemberInfoMedium" parent="TextAppearance.AppCompat.Medium">
        <item name="android:textColor">@android:color/primary_text_light</item>
    </style>

    <style name="MembersListPrimary" parent="TextAppearance.AppCompat.Medium">
        <item name="android:ellipsize">end</item>
        <item name="android:textColor">@android:color/primary_text_light</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="MembersListSecondary" parent="TextAppearance.AppCompat.Small">
        <item name="android:ellipsize">end</item>
        <item name="android:textColor">@android:color/primary_text_light</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="MembersListTertiary" parent="TextAppearance.AppCompat.Small">
        <item name="android:singleLine">true</item>
    </style>

    <style name="OnboardingLabel" parent="TextAppearance.AppCompat.Medium">
        <item name="android:textColor">@android:color/primary_text_light</item>
    </style>

    <style name="SettingsLabel" parent="TextAppearance.AppCompat.Medium">
        <item name="android:textColor">@android:color/primary_text_light</item>
    </style>

    <style name="Crop"></style>

    <style name="Crop.DoneCancelBar">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/crop__bar_height</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:divider">@drawable/crop__divider</item>
        <item name="android:showDividers">middle</item>
        <item name="android:dividerPadding">12dp</item>
        <item name="android:background">#f3f3f3</item>
    </style>

    <style name="Crop.ActionButton">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">@drawable/crop__selectable_background</item>
    </style>

    <style name="Crop.ActionButtonText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingRight">20dp</item> <!-- Offsets left drawable -->
        <item name="android:drawablePadding">8dp</item>
        <item name="android:textColor">#666666</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">13sp</item>
    </style>

    <style name="Crop.ActionButtonText.Done">
        <item name="android:drawableLeft">@drawable/crop__ic_done</item>
        <item name="android:text">@string/crop__done</item>
    </style>

    <style name="Crop.ActionButtonText.Cancel">
        <item name="android:drawableLeft">@drawable/crop__ic_cancel</item>
        <item name="android:text">@string/crop__cancel</item>
    </style>
</resources>
